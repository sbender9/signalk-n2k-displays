{
  "name": "signalk-n2k-displays",
  "version": "2.0.0",
  "description": "Signal K Plugin ",
  "main": "index.js",
  "scripts": {
    "format": "prettier-standard 'src/*.ts'",
    "build": "tsc --pretty false",
    "watch": "npm run build -- -w",
