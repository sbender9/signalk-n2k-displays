    "camelcase": "^6.2.0",
    "semver": "^7.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sbender9/signalk-n2k-displays"
  },
  "devDependencies": {
    "@signalk/github-create-release": "^1.2.1",
    "@types/node": "^14.14.10",
    "@types/semver": "^7.7.0",
    "prettier-standard": "^16.4.1",
    "typescript": "^4.1.2"
  },
  "signalk-plugin-enabled-by-default": true
